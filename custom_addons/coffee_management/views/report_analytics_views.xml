        <record id="view_coffee_arrival_pivot" model="ir.ui.view">
            <field name="name">coffee.arrival.pivot</field>
            <field name="model">coffee.arrival</field>
            <field name="arch" type="xml">
                <pivot string="Coffee Arrivals" disable_linking="True">
                    <field name="supplier_id" type="row"/>
                    <field name="date" type="col"/>
                    <field name="coffee_issue_no"/>
                    <field name="product_id" type="measure"/>
                </pivot>
            </field>
        </record>

        <record id="view_coffee_arrival_graph" model="ir.ui.view">
            <field name="name">coffee.arrival.graph</field>
            <field name="model">coffee.arrival</field>
            <field name="arch" type="xml">
                <graph string="Coffee Arrivals" type="bar">
                    <field name="supplier_id" type="measure"/>
                    <field name="coffee_issue_no"/>
                </graph>
            </field>
        </record>

